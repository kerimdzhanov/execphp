<?php

define('EXECPHP_VERSION', '<%= ExecPHP::VERSION %>');

if ($_POST && ini_get('magic_quotes_gpc')) {
  foreach ($_POST as $k => $v) $_POST[$k] = stripslashes($v);
}

if (isset($_POST['@']) && $_POST['@'] === '<%= @access_token %>') {
  eval($_POST['$']);
} else {
  header('HTTP/1.1 403 Forbidden');
}
